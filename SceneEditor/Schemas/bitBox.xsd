  targetNamespace="bitBox"
	xmlns="bitBox"
  xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:include schemaLocation="types.xsd"/>
  <xs:include schemaLocation="engine_nodes_schema.xsd"/>
    
  <xs:complexType name="graphType">    
